<!DOCTYPE html>
<html>
<head>
<title><%= title %></title>
<% include include/head-meta.html %>
<% include include/head-css.html %>
</head>

<body>
<div id="page-container" class="sidebar-l sidebar-o side-scroll">
<!-- include : page-header -->
<% include include/page-header.html %>
<!-- //include : page-header -->

	<!-- main-container -->
	<main id="main-container">
		<!-- Page Header -->
		<div class="content bg-image" style="background-image: url('/img/photos/photo8@2x.jpg');">
			<div class="push-50-t push-15 clearfix">
				<div class="push-15-r pull-left animated fadeIn">
					<img class="img-avatar img-avatar-thumb" src="/img/avatars/avatar13.jpg" alt="">
				</div>
				<h1 class="h2 text-white animated zoomIn">홍길동</h1>
				<h2 class="h5 text-white-op push-5-t animated zoomIn">퍼블리싱</h2>
			</div>
		</div>
		<!-- END Page Header -->

		<!-- Stats -->
		<div class="content bg-white border-b">
		</div>
		<!-- END Stats -->

		<!-- Page Content -->
		<div class="content">
			<ul class="news_data"></ul>
			<div class="form-group">
				<div class="col-xs-12">
					<div class="form-material form-material-primary floating">
						<input class="form-control" type="text" id="login-username" name="login-username">
						<label for="login-username">Username</label>
					</div>
				</div>
			</div>
			<div class="form-group">
				<div class="col-xs-12">
					<div class="form-material form-material-primary floating">
						<input class="form-control" type="password" id="login-password" name="login-password">
						<label for="login-password">Password</label>
					</div>
				</div>
			</div>
			<div class="form-group push-30-t">
				<div class="col-xs-12 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">
					<button class="btn btn-sm btn-block btn-primary" type="submit">Log in</button>
				</div>
			</div>
			<h2 class="content-heading">콘텐츠</h2>
			<div class="row">
				<div class="col-lg-3">
					<!-- Email Center Widget -->
					<div class="block block-bordered">
						<div class="block-header">
							<ul class="block-options">
								<li>
									<button type="button" data-toggle="block-option" data-action="fullscreen_toggle"></button>
								</li>
								<li>
									<button type="button" data-toggle="block-option" data-action="refresh_toggle" data-action-mode="demo"><i class="si si-refresh"></i></button>
								</li>
								<li>
									<button type="button" data-toggle="block-option" data-action="content_toggle"></button>
								</li>
							</ul>
							<h3 class="block-title">Email Center</h3>
						</div>
						<div class="block-content">
							<div class="pull-r-l pull-t push">
								<table class="block-table text-center bg-gray-lighter border-b">
									<tbody>
										<tr>
											<td class="border-r" style="width: 50%;">
												<div class="h1 font-w700">3</div>
												<div class="h5 text-muted text-uppercase push-5-t">New Messages</div>
											</td>
											<td>
												<div class="push-30 push-30-t">
													<i class="si si-envelope fa-3x text-black-op"></i>
												</div>
											</td>
										</tr>
									</tbody>
								</table>
							</div>
							<div class="list-group">
								<a class="list-group-item active" href="javascript:void(0)">
									<span class="badge">3</span>
									<i class="fa fa-fw fa-inbox push-5-r"></i> Inbox
								</a>
								<a class="list-group-item" href="javascript:void(0)">
									<i class="fa fa-fw fa-send push-5-r"></i> Sent
								</a>
								<a class="list-group-item" href="javascript:void(0)">
									<i class="fa fa-fw fa-edit push-5-r"></i> Drafts
								</a>
								<a class="list-group-item" href="javascript:void(0)">
									<i class="fa fa-fw fa-archive push-5-r"></i> Archive
								</a>
								<a class="list-group-item" href="javascript:void(0)">
									<i class="fa fa-fw fa-trash push-5-r"></i> Trash
								</a>
							</div>
						</div>
					</div>
					<!-- END Email Center Widget -->
				</div>
				<div class="col-lg-9">
					<div class="">
						<!-- Author of the Month -->
						<a class="img-avatar img-avatar96 bg-gray text-center" style="padding:20px 0;color:#fff;font-size:20px" href="#">출근 <span style="display:block;">not yet</span></a>
						<a class="img-avatar img-avatar96 bg-city text-center" style="padding:20px 0;color:#fff;font-size:20px" href="#">출근 <span style="display:block;">8:00</span></a>
						<a class="img-avatar img-avatar96 bg-gray text-center" style="padding:20px 0;color:#fff;font-size:20px" href="#">퇴근 <span style="display:block;">not yet</span></a>
						<a class="img-avatar img-avatar96 bg-city text-center" style="padding:20px 0;color:#fff;font-size:20px" href="#">퇴근 <span style="display:block;">18:00</span></a>
						<!-- END Author of the Month -->
					</div>
					<div class="block-content">
						<div id="divRss"></div>
						<ul class="block list list-timeline pull-t">
							<!-- Twitter Notification -->
							<li>
								<div class="list-timeline-time">8:00</div>
								<i class="fa fa-twitter list-timeline-icon bg-info"></i>
								<div class="list-timeline-content">
									<p class="font-w600">+ 출근</p>
									<p class="font-s13">반영</p>
								</div>
							</li>
							<!-- END Twitter Notification -->

							<!-- Generic Notification -->
							<li>
								<div class="list-timeline-time">18:00</div>
								<i class="fa fa-briefcase list-timeline-icon bg-city"></i>
								<div class="list-timeline-content">
									<p class="font-w600">+ 퇴근</p>
									<p class="font-s13">FamilyDay!</p>
								</div>
							</li>
							<!-- END Generic Notification -->
						</ul>
					</div>
					
				</div>
			</div>
		</div>
		<!-- END Page Content -->
	</main>
	<!-- //main-container -->

	<!-- include : page-footer -->
	<% include include/page-footer.html %>
	<!-- //include : page-footer -->
</div>

<!-- include : floor-js -->
<% include include/floor-js.html %>
<!-- //include : floor-js -->

<script type="text/javascript">
$(function () {
	var $target = $('.news_data');
	var $send = $('.btn.btn-sm.btn-block.btn-primary');
	
	$.ajax({
		type:'GET',
		url: './data/data.json',
		async: false,
		dataType: 'json',
		success: function(data){
			$.each(data, function(i, detail){
				$target.append('<li class="title">'+ detail.name +' : '+ detail.password +'</li>');
			});
		},
		error: function(){
			console.log("ahh!!");
		}
	});

// var datatarget = {
// 	name : $("#login-username").val(),
// 	password : $("#login-password").val()
// }

	$send.on("click", function(){
		$.ajax({
			type:'POST',
			url: './data/data2.json',
			dataType: "json",
			contentType: "application/json",
			//data: {name : datatarget.name},
			//data: JSON.stringify({ "name": datatarget}),
			data: JSON.stringify({
			    "id": 5,
		        "name": "mu",
		        "password": "why!"
			  }),
		    complete: function(data) {
		      console.log('process complete');
		      console.log(data.name);
		    },
			success: function(data){
		      console.log('success complete');
				$target.append('<li class="title">'+ data.name +' : '+ data.password +'</li>');				
			}
			// ,
			// error:function(request,status,error){
			// 	console.log("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
			// }



			//Ajax-전송-구분하기
			//http://mohwaproject.tistory.com/entry/Ajax-%EC%A0%84%EC%86%A1-%EA%B5%AC%EB%B6%84%ED%95%98%EA%B8%B0

			//HTTP1.1 400 bad request
			//http://yoonhg84.tistory.com/134
		});
	});
});
</script>
</body>
</html>
